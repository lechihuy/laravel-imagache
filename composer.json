{
    "name": "lechihuy/laravel-imagache",
    "description": "Suport API for cache images in Laravel framework.",
    "homepage": "https://github.com/lechihuy/laravel-imagache",
    "authors": [
        {
            "name": "Lê Chí Huy",
            "email": "lechihuy1062001@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "intervention/image": "^2.7",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Imagache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Imagache\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Imagache\\ImagacheServiceProvider"
            ]
        }
    }
}
