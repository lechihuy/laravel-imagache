{
    "name": "vendor/package",
    "description": "The skeleton of the package in Laravel framework.",
    "homepage": "https://domain.com",
    "authors": [
        {
            "name": "Lê Chí Huy",
            "email": "lechihuy1062001@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Package\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Package\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Package\\PackageServiceProvider"
            ]
        }
    }
}